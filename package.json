{
  "name": "pomodoro-timer-vscode",
  "displayName": "pomodoro-timer-vscode",
  "description": "Hello, world!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pomodoro-timer-vscode.helloPomodoro"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
        {
            "command": "pomodoro-timer-vscode.helloPomodoro",
            "title": "Hello Pomodoro"
        },
        {
            "command": "pomodoro-timer-vscode.startPomodoro",
            "title": "Start Pomodoro"
        },
        {
            "command": "pomodoro-timer-vscode.pausePomodoro",
            "title": "Pause Pomodoro"
        },
        {
            "command": "pomodoro-timer-vscode.playPomodoro",
            "title": "Play Pomodoro"
        }

    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
